    <!-- Прямоугольник из которого вылетают частицы -->
    <!-- Координаты левого-верхнего угла прямоугольника [вещественная] -->
    <StartRectanglePosition>0 0</StartRectanglePosition>
    <!-- Размер левого-верхнего угла прямоугольника (в пикселах) [вещественная] -->
    <StartRectangleSize>64 24</StartRectangleSize>
    <!-- Количество вылетающих частиц в секунду [вещественная] -->
    <ParticlesPerSecond>15</ParticlesPerSecond>
    <!-- Коэффициент трения частицы [вещественная] -->
    <ParticleFriction>0.9987</ParticleFriction>
  </Asset>
