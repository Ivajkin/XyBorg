    <!-- Разброс угла вылета частиц [вещественная] -->
    <StartParticleVelocityAngleRandom>0.63</StartParticleVelocityAngleRandom>
    <!-- Разброс скорости вылета частиц (от 1 до показателя) [вещественная] -->
